package in3.a10;

import java.io.PrintWriter;

import lernhilfe2013ss.event.Timer;
import lernhilfe2013ws.graphics.DirtyPainter;

public class CommandTest {

	public static void main(String[] args) {

		PrintWriter pw = new PrintWriter(System.out, true);
		
		// AmpelSteuerung steuerung = new AmpelSteuerung();
		ActionPrinter ap = new ActionPrinter(pw);
		
		Timer timer = new Timer(1000);
		timer.addActionListener(ap);
		timer.setActionCommand(Command.NEXT_PHASE);
		timer.start();
		Thread.sleep(10000);
	}

}
