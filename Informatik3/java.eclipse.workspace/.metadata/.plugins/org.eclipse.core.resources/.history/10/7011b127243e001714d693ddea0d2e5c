package in3.a07;

public class Person {

	protected String vorname;
	protected String nachname;
	protected int geburtsjahr;
	
	
	public Person(String vorname, String nachname, int geburtsjahr){
		this.vorname = vorname;
		this.nachname = nachname;
		this.geburtsjahr = geburtsjahr;
	}
	
	public Person(Person p){
		this(p.vorname, p.nachname, p.geburtsjahr);
	}
	
	public Person(lernhilfe2013ws.objectPlay.Person p){
		this.vorname = p.getVorname();
		this.nachname = p.getNachname();
		this.geburtsjahr = p.getGeburtsjahr();
	}
}
